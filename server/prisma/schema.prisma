
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model User {
  user_id       String @id @default(auto()) @map("_id") @db.ObjectId
  user_name     String
  user_email    String  @unique
  user_mobile   String  @unique
  user_password String
}

